<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <notice>

        Welcome to the Nova Engine source tree. Please enjoy and respect the peace.

    </notice>
    <remote name="nova-engine"
        fetch="https://review.r3t.io/"
        review="https://review.r3t.io/" />

    <default revision="mainline" sync-j="4" />

    <project path="manifest" remote="nova-engine" name="nova/manifest" />
    <project path="pleiades" remote="nova-engine" name="nova/pleiades" groups="pleiades" />
    <project path="api" remote="nova-engine" name="nova/api" groups="pleiades" />
    <project path="raft" remote="nova-engine" name="nova/raft" groups="pleiades">
        <linkfile src="." dest="pleiades/raft" />
    </project>
</manifest>
